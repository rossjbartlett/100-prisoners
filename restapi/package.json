{
  "name": "restapi",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 12.13 <13"
  },
  "main": "./build/src/main.js",
  "scripts": {
    "prod": "node ./build/src/main.js",
    "start": "nodemon --watch ./build --inspect=0.0.0.0:5858 --nolazy ./build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:watch:output": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "postinstall": "tsc -p tsconfig.release.json",
    "compile-schemas": "compile-schemas-to-typescript src/routes/schemas src/routes/types"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "fastify": "^3.14.2",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^5.3.1",
    "fastify-swagger": "^4.5.0",
    "middie": "^5.2.0",
    "pg": "^8.7.3",
    "superagent": "^6.1.0",
    "tslib": "~1.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "~25.2.1",
    "@types/node": "~12.12.22",
    "@types/pg": "^8.6.5",
    "@types/superagent": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "~2.30.0",
    "@typescript-eslint/parser": "~2.30.0",
    "compile-schemas-to-typescript": "^0.2.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-plugin-functional": "^3.0.1",
    "eslint-plugin-jest": "~23.8.2",
    "jest": "~25.5.2",
    "nodemon": "^2.0.3",
    "prettier": "~2.0.5",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.4.0",
    "tsutils": "~3.17.1",
    "typescript": "^4.7.4"
  }
}
